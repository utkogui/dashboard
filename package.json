{
  "name": "dash_ftd",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "tsx server/index.ts",
    "dev:full": "concurrently \"npm run server\" \"npm run dev\"",
    "build": "prisma generate && tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "neon:config": "node scripts/setup-neon.js",
    "neon:export": "node scripts/export-from-sqlite.js",
    "neon:migrate": "npx prisma migrate dev --name initial",
    "neon:import": "node scripts/import-to-neon.js",
    "neon:test": "node scripts/test-neon-connection.js",
    "neon:setup": "npm run neon:config && npm run neon:export && npm run neon:migrate && npm run neon:import && npm run neon:test",
    "sqlite:restore": "node scripts/restore-sqlite.js && npx prisma generate",
    "db:export": "node scripts/migrate-to-postgres.js",
    "db:setup": "node scripts/setup-postgres.js",
    "db:import": "node scripts/import-to-postgres.js",
    "db:test": "node scripts/test-postgres-connection.js",
    "db:migrate": "npm run db:export && npm run db:setup && npm run db:import && npm run db:test"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^6.1.7",
    "@mui/material": "^6.1.7",
    "@mui/x-data-grid": "^8.9.2",
    "@prisma/adapter-pg": "^6.14.0",
    "@prisma/client": "^6.13.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/react-input-mask": "^3.0.6",
    "@vitejs/plugin-react": "^4.3.4",
    "antd": "^5.26.7",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^12.2.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "dhtmlx-gantt": "^9.0.14",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.536.0",
    "pg": "^8.16.3",
    "primeflex": "^4.0.0",
    "primeicons": "^7.0.0",
    "primereact": "^10.9.6",
    "prisma": "^6.13.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-input-mask": "^2.0.4",
    "react-router-dom": "^7.1.1",
    "recharts": "^2.12.7",
    "tsx": "^4.20.3",
    "vite": "^5.4.10",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "concurrently": "^9.1.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^16.3.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.35.1"
  }
}
